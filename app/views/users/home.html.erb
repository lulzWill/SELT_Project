<h1>Welcome <%=@current_user.user_id%></h1>

<% if @current_user.role != "Admin" %>
<%= link_to "Calendar", calendar_path, method: :get%>
<% end %>

<h2>Courses</h2>
<% puts @current_user.courses %>
<% for course in @current_user.courses%>
<%= link_to "#{course.name}", course_path(course)%>
<br>
<% end %>
<% if @current_user.role != "Student" %>
<%= link_to "View Students", view_students_path%>
<br>
<% end %>
<%= link_to "Enroll in a course", courses_path%>