<h1>Welcome <%@user.user_id%></h1>

<% if @user.role != "Admin" %>
<div id='calendar'></div>
<% end %>

<p>Courses</p>
<% for course in @courses%>
<%= link_to "#{course.name}", course_path(course)%>
<br>
<% end %>
<% if @user.role == "Professor" %>
<%= link_to "View Students", view_students_path%>
<br>
<% end %>